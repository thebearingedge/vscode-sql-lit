{
  "scopeName": "inline.sql-lit",
  "fileTypes": [],
  "injectionSelector": "L:source -comment -string, L:markup.fenced_code.block.markdown (meta.embedded.block.javascript, meta.embedded.block.typescript, meta.embedded.block.typescriptreact, meta.embedded.block.vue)",
  "patterns": [
    {
      "include": "#sql-lit-tag"
    },
    {
      "include": "#sql-lit-property-tag"
    },
    {
      "include": "#sql-lit-higher-order-tag"
    },
    {
      "include": "#sql-lit-comment"
    }
  ],
  "repository": {
    "sql-lit-tag": {
      "name": "meta.embedded.block.sql",
      "begin": "(?i)(?:sql|db)\\w*\\s*(<.+>)?\\s*(`)",
      "beginCaptures": {
        "0": {
          "name": "entity.name.function.tagged-template.js"
        },
        "1": {
          "patterns": [
            {
              "include": "source.ts#type-arguments"
            }
          ]
        },
        "2": {
          "name": "punctuation.definition.string.template.begin.js string.template.js"
        }
      },
      "end": "(`)",
      "endCaptures": {
        "0": {
          "name": "string.js"
        },
        "1": {
          "name": "punctuation.definition.string.template.end.js"
        }
      },
      "patterns": [
        {
          "include": "source.ts#template-substitution-element"
        },
        {
          "include": "source.plpgsql.postgres"
        },
        {
          "include": "source.sql"
        }
      ]
    },
    "sql-lit-property-tag": {
      "name": "meta.embedded.block.sql",
      "begin": "(?i)(?:sql|db)\\w*\\s*\\.([_$[:alpha:]][_$[:alnum:]]*)?\\s*(<.+>)?\\s*(`)",
      "beginCaptures": {
        "1": {
          "name": "entity.name.function.tagged-template.js"
        },
        "2": {
          "patterns": [
            {
              "include": "source.ts#type-arguments"
            }
          ]
        },
        "3": {
          "name": "punctuation.definition.string.template.begin.js string.template.js"
        }
      },
      "end": "(`)",
      "endCaptures": {
        "0": {
          "name": "string.js"
        },
        "1": {
          "name": "punctuation.definition.string.template.end.js"
        }
      },
      "patterns": [
        {
          "include": "source.ts#template-substitution-element"
        },
        {
          "include": "source.plpgsql.postgres"
        },
        {
          "include": "source.sql"
        }
      ]
    }
  }
}
